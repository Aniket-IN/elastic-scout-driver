{
    "name": "babenkoivan/elastic-scout-driver",
    "description": "Elasticsearch driver for Laravel Scout",
    "keywords": [
        "scout",
        "elastic",
        "elasticsearch",
        "laravel",
        "driver",
        "php"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Babenko",
            "email": "babenko.i.a@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ElasticScoutDriver\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElasticScoutDriver\\Tests\\": "tests"
        }
    },
    "require": {
        "babenkoivan/elastic-client": "^1.0",
        "babenkoivan/elastic-adapter": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "^4.5",
        "babenkoivan/elastic-migrations": "^1.0",
        "laravel/scout": "^7.2"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ElasticScoutDriver\\ServiceProvider"
            ]
        }
    }
}
